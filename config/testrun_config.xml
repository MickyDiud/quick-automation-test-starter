<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Sample Demo" verbose="0" parallel="tests" thread-count="100">

	<listeners>
		<listener class-name="com.quantum.listeners.QuantumReportiumListener" />
	</listeners>

	<test name="Web Scenarios OpenSea Test" enabled="true">
		<groups>
			<run>
				<include name="@WebTest" />
				<include name="@TearDown" />
			</run>
		</groups>
		<classes>
			<class name="com.qmetry.qaf.automation.step.client.gherkin.GherkinScenarioFactory" />
			<class name="com.qmetry.qaf.automation.step.client.text.BDDTestFactory" />
			<class name="com.qmetry.qaf.automation.step.client.csv.KwdTestFactory" />
		</classes>
	</test>
</suite>