+                TimeUnit.SECONDS.sleep(2);
+                try {
+                    action.keyDown(Keys.CONTROL).click(gridCell).keyUp(Keys.CONTROL).perform();
+                } catch (Exception e) {
+                    // Fix:
+                    if (gridCell.isPresent()) {
